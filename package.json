{
	"name": "cool-retro-term-webgl-monorepo",
	"version": "1.0.0",
	"private": true,
	"description": "Monorepo for cool-retro-term-webgl packages",
	"workspaces": [
		"modules/cool-retro-term-renderer",
		"modules/cool-retro-term-electron"
	],
	"scripts": {
		"build": "npm run build --workspaces",
		"build:renderer": "npm run build --workspace=cool-retro-term-renderer",
		"build:electron": "npm run build --workspace=cool-retro-term-electron",
		"dev:renderer": "npm run dev --workspace=cool-retro-term-renderer",
		"dev:electron": "npm run dev --workspace=cool-retro-term-electron",
		"start:electron": "npm run start --workspace=cool-retro-term-electron",
		"dist": "npm run dist --workspace=cool-retro-term-electron",
		"dist:mac": "npm run dist:mac --workspace=cool-retro-term-electron",
		"dist:win": "npm run dist:win --workspace=cool-retro-term-electron",
		"dist:linux": "npm run dist:linux --workspace=cool-retro-term-electron",
		"lint-and-format": "npx @biomejs/biome check --write --unsafe"
	},
	"author": "Remo H. Jansen",
	"license": "GPL-3.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/remojansen/cool-retro-term-webgl"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4"
	}
}
